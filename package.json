{
  "name": "intune-device-remover",
  "version": "1.0.0",
  "description": "",
  "main": "electron-main.js",
  "scripts": {
    "start": "electron .",
    "make": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "archiver": "^7.0.1",
    "axios": "^1.10.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "xlsx": "^0.18.5"
  },
  "build": {
    "productName": "Intune Device Remover",
    "appId": "com.skolden.intune-device-remover",
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "files": [
      "cli.js",
      "electron-main.js",
      "core.js",
      "preload.js",
      "renderer.js",
      "index.html",
      "assets/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "assets/managedClientSummaryReport.png",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "arm64"
          ]
        }
      ],
      "identity": null,
      "hardenedRuntime": false,
      "gatekeeperAssess": false
    },
    "win": {
      "icon": "assets/managedClientSummaryReport.png",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^37.1.0",
    "electron-builder": "^26.0.12"
  }
}